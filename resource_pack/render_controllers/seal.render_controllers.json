{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.seal": {
      "arrays": {
        "textures": {
          "Array.skin_variants": [
            "Texture.default",
            "Texture.arctic",
            "Texture.brown",
            "Texture.plant"
          ],
          "Array.saddle_overlay": ["Texture.transparent", "Texture.saddle"]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "Array.skin_variants[query.variant]",
        "Array.saddle_overlay[query.is_saddled]"
      ]
    }
  }
}
