{
  "format_version": "1.21.0",
  "minecraft:entity": {
    "description": {
      "identifier": "my:seal",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false,
      "properties": {
        "my:is_tickled": {
          "type": "bool",
          "default": false,
          "client_sync": true
        },
        "my:is_anchored": {
          "type": "bool",
          "default": false,
          "client_sync": true
        },
        "my:is_clapping": {
          "type": "bool",
          "default": false,
          "client_sync": true
        },
        "my:is_rolling": {
          "type": "bool",
          "default": false,
          "client_sync": true
        }
      }
    },

    "component_groups": {
      "my:variant_gray": {
        "minecraft:variant": { "value": 0 }
      },
      "my:variant_arctic": {
        "minecraft:variant": { "value": 1 }
      },
      "my:variant_brown": {
        "minecraft:variant": { "value": 2 }
      },
      "my:variant_plant": {
        "minecraft:variant": { "value": 3 }
      },

      "my:wild": {
        "minecraft:behavior.nearest_attackable_target": {
          "priority": 4,
          "must_see": true,
          "reselect_targets": true,
          "within_radius": 20.0,
          "must_see_forget_duration": 17.0,
          "entity_types": [
            {
              "filters": {
                "all_of": [
                  {
                    "test": "in_water",
                    "subject": "other",
                    "value": true
                  },
                  {
                    "any_of": [
                      {
                        "test": "is_family",
                        "subject": "other",
                        "value": "cod"
                      },
                      {
                        "test": "is_family",
                        "subject": "other",
                        "value": "salmon"
                      },
                      {
                        "test": "is_family",
                        "subject": "other",
                        "value": "tropicalfish"
                      }
                    ]
                  }
                ]
              },
              "max_dist": 16
            }
          ]
        },
        "minecraft:behavior.move_to_water": {
          "priority": 5,
          "search_range": 16,
          "search_height": 5,
          "search_count": 1,
          "goal_radius": 0.1
        },
        "minecraft:attack": {
          "damage": 2
        },
        "minecraft:behavior.melee_attack": {
          "priority": 5,
          "speed_multiplier": 2.0,
          "track_target": true
        }
      },

      "my:tamed": {
        "minecraft:is_tamed": {},
        "minecraft:healable": {
          "items": [
            { "item": "minecraft:cod_bucket", "heal_amount": 6 },
            { "item": "minecraft:salmon_bucket", "heal_amount": 6 },
            { "item": "minecraft:tropical_fish_bucket", "heal_amount": 6 }
          ]
        },
        "minecraft:behavior.panic": {
          "priority": 1,
          "speed_multiplier": 1.5
        }
      },

      "my:tamed_inventory": {
        "minecraft:inventory": {
          "container_type": "horse",
          "inventory_size": 16
        },
        "minecraft:equippable": {
          "slots": [
            {
              "slot": 0,
              "item": "minecraft:saddle",
              "accepted_items": ["minecraft:saddle"],
              "on_equip": {
                "event": "my:equip_saddle",
                "target": "self"
              },
              "on_unequip": {
                "event": "my:unequip_saddle",
                "target": "self"
              }
            }
          ]
        }
      },

      "my:unchested": {
        "minecraft:interact": {
          "interactions": [
            {
              "play_sounds": "armor.equip_generic",
              "on_interact": {
                "filters": {
                  "all_of": [
                    {
                      "test": "is_sneaking",
                      "subject": "other",
                      "value": false
                    },
                    {
                      "test": "has_equipment",
                      "domain": "hand",
                      "subject": "other",
                      "value": "chest"
                    }
                  ]
                },
                "event": "my:on_chest",
                "target": "self"
              },
              "use_item": true,
              "interact_text": "action.interact.attachchest"
            }
          ]
        }
      },

      "my:chested": {
        "minecraft:is_chested": {},
        "minecraft:behavior.nearest_attackable_target": {
          "priority": 4,
          "must_see": true,
          "reselect_targets": true,
          "within_radius": 20.0,
          "must_see_forget_duration": 17.0,
          "entity_types": [
            {
              "filters": {
                "all_of": [
                  {
                    "test": "in_water",
                    "subject": "other",
                    "value": true
                  },
                  {
                    "none_of": [
                      {
                        "test": "is_family",
                        "subject": "other",
                        "value": "player"
                      },
                      {
                        "test": "is_family",
                        "subject": "other",
                        "value": "seal"
                      }
                    ]
                  },
                  {
                    "any_of": [
                      {
                        "test": "is_family",
                        "subject": "other",
                        "value": "cod"
                      },
                      {
                        "test": "is_family",
                        "subject": "other",
                        "value": "salmon"
                      },
                      {
                        "test": "is_family",
                        "subject": "other",
                        "value": "tropicalfish"
                      },
                      {
                        "test": "is_family",
                        "subject": "other",
                        "value": "pufferfish"
                      }
                    ]
                  }
                ]
              },
              "max_dist": 16
            }
          ]
        },
        "minecraft:attack": {
          "damage": 2
        },
        "minecraft:behavior.melee_attack": {
          "priority": 5,
          "speed_multiplier": 2.0,
          "track_target": true
        }
      },

      "my:rideable": {
        "minecraft:rideable": {
          "seat_count": 1,
          "crouching_skip_interact": true,
          "family_types": ["player"],
          "interact_text": "action.interact.ride",
          "seats": [
            {
              "position": [0.0, 0.6, -0.2]
            }
          ]
        }
      },

      "my:saddled": {
        "minecraft:is_saddled": {},
        "minecraft:rideable": {
          "seat_count": 1,
          "crouching_skip_interact": true,
          "family_types": ["player"],
          "interact_text": "action.interact.ride",
          "seats": [
            {
              "position": [0.0, 0.6, -0.2]
            }
          ],
          "controlling_seat": 0
        },
        "minecraft:behavior.player_ride_tamed": {
          "priority": 0
        }
      },

      "my:saddled_in_water": {
        "minecraft:free_camera_controlled": {
          "strafe_speed_modifier": 0.7,
          "backwards_movement_modifier": 0.5
        },
        "minecraft:dash_action": {
          "cooldown_time": 3,
          "horizontal_momentum": 15.0,
          "vertical_momentum": 0.05,
          "can_dash_underwater": true,
          "direction": "passenger"
        },
        "minecraft:environment_sensor": {
          "triggers": [
            {
              "filters": {
                "test": "in_water",
                "value": false
              },
              "event": "my:saddled_leave_water"
            }
          ]
        }
      },

      "my:saddled_on_ground": {
        "minecraft:input_ground_controlled": {},
        "minecraft:dash_action": {
          "cooldown_time": 3,
          "horizontal_momentum": 10.0,
          "vertical_momentum": 0.05,
          "can_dash_underwater": false,
          "direction": "passenger"
        },
        "minecraft:environment_sensor": {
          "triggers": [
            {
              "filters": {
                "test": "in_water"
              },
              "event": "my:saddled_enter_water"
            }
          ]
        }
      },

      "my:adult": {
        "minecraft:breedable": {
          "require_tame": true,
          "require_full_health": true,
          "breed_items": [
            "minecraft:cod_bucket",
            "minecraft:salmon_bucket",
            "minecraft:tropical_fish_bucket"
          ],
          "breeds_with": [
            {
              "mate_type": "my:seal",
              "baby_type": "my:seal",
              "breed_event": {
                "event": "minecraft:entity_born",
                "target": "baby"
              }
            }
          ]
        },
        "minecraft:behavior.breed": {
          "priority": 2,
          "speed_multiplier": 1.0
        }
      },

      "my:baby": {
        "minecraft:is_baby": {},
        "minecraft:ageable": {
          "duration": 300,
          "feed_items": [
            { "item": "minecraft:cod_bucket", "growth": 0.1 },
            { "item": "minecraft:salmon_bucket", "growth": 0.1 },
            { "item": "minecraft:tropical_fish_bucket", "growth": 0.1 }
          ],
          "grow_up": {
            "event": "my:grow_up",
            "target": "self"
          }
        },
        "minecraft:scale": {
          "value": 0.5
        }
      },

      "my:tamed_anchored": {
        "minecraft:home": {
          "restriction_radius": 16,
          "restriction_type": "random_movement"
        }
      },

      "my:detect_biome": {
        "minecraft:environment_sensor": {
          "triggers": [
            {
              "filters": {
                "any_of": [
                  { "test": "has_biome_tag", "value": "warm" },
                  { "test": "has_biome_tag", "value": "lukewarm" }
                ]
              },
              "event": "my:set_warm_variant"
            },
            {
              "filters": {
                "any_of": [
                  { "test": "has_biome_tag", "value": "frozen" },
                  { "test": "has_biome_tag", "value": "ice" }
                ]
              },
              "event": "my:set_cold_variant"
            },
            {
              "filters": {
                "all_of": [
                  {
                    "any_of": [
                      { "test": "has_biome_tag", "value": "ocean" },
                      { "test": "has_biome_tag", "value": "beach" }
                    ]
                  },
                  {
                    "test": "has_biome_tag",
                    "operator": "!=",
                    "value": "warm"
                  },
                  {
                    "test": "has_biome_tag",
                    "operator": "!=",
                    "value": "lukewarm"
                  },
                  {
                    "test": "has_biome_tag",
                    "operator": "!=",
                    "value": "frozen"
                  }
                ]
              },
              "event": "my:set_temperate_variant"
            }
          ]
        },
        "minecraft:timer": {
          "time": 0.5,
          "looping": false,
          "time_down_event": {
            "event": "my:set_random_variant"
          }
        }
      }
    },

    "components": {
      "minecraft:type_family": {
        "family": ["seal", "animal", "mob"]
      },
      "minecraft:health": {
        "value": 30,
        "max": 30
      },
      "minecraft:collision_box": {
        "width": 1.2,
        "height": 0.8
      },
      "minecraft:loot": {
        "table": "loot_tables/entities/seal.json"
      },
      "minecraft:physics": {
        "has_gravity": true,
        "has_collision": true
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0,
        "breathes_water": true,
        "breathes_air": true,
        "generates_bubbles": false
      },
      "minecraft:navigation.generic": {
        "is_amphibious": true,
        "can_path_over_water": true,
        "can_swim": true,
        "can_walk": true,
        "can_sink": false,
        "avoid_water": false,
        "can_path_from_air": true
      },
      "minecraft:movement.amphibious": {
        "max_turn": 30.0
      },
      "minecraft:movement": {
        "value": 0.18
      },
      "minecraft:underwater_movement": {
        "value": 0.12
      },
      "minecraft:jump.static": {},
      "minecraft:can_climb": {},
      "minecraft:nameable": {},
      "minecraft:home": {},
      "minecraft:leashable": {
        "soft_distance": 4.0,
        "hard_distance": 6.0,
        "max_distance": 10.0
      },
      "minecraft:experience_reward": {
        "on_death": "query.last_hit_by_player ? math.random(1,3) : 0"
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "fall",
            "deals_damage": false
          }
        ]
      },

      "minecraft:behavior.tempt": {
        "priority": 3,
        "speed_multiplier": 1.2,
        "can_tempt_vertically": true,
        "items": [
          "minecraft:cod_bucket",
          "minecraft:salmon_bucket",
          "minecraft:tropical_fish_bucket"
        ]
      },
      "minecraft:behavior.swim_idle": {
        "priority": 7,
        "idle_time": 5.0,
        "success_rate": 0.05
      },
      "minecraft:behavior.random_swim": {
        "priority": 8,
        "speed_multiplier": 1.0,
        "interval": 0,
        "xz_dist": 30,
        "y_dist": 15
      },
      "minecraft:behavior.random_stroll": {
        "priority": 9,
        "speed_multiplier": 1.2,
        "interval": 40
      },
      "minecraft:behavior.look_at_player": {
        "priority": 10,
        "look_distance": 8.0
      },
      "minecraft:behavior.random_look_around": {
        "priority": 11
      }
    },

    "events": {
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": ["my:wild", "my:detect_biome", "my:rideable"]
        }
      },

      "minecraft:entity_born": {
        "add": {
          "component_groups": ["my:baby", "my:tamed", "my:detect_biome"]
        },
        "set_home_position": {}
      },

      "my:on_tame": {
        "sequence": [
          { "remove": { "component_groups": ["my:wild"] } },
          {
            "add": {
              "component_groups": [
                "my:tamed",
                "my:tamed_inventory",
                "my:unchested",
                "my:adult"
              ]
            }
          },
          { "set_home_position": {} }
        ]
      },

      "my:on_chest": {
        "remove": { "component_groups": ["my:unchested"] },
        "add": { "component_groups": ["my:chested"] }
      },

      "my:equip_saddle": {
        "sequence": [
          { "remove": { "component_groups": ["my:rideable"] } },
          { "add": { "component_groups": ["my:saddled"] } },
          {
            "filters": { "test": "in_water" },
            "add": { "component_groups": ["my:saddled_in_water"] }
          },
          {
            "filters": { "test": "in_water", "value": false },
            "add": { "component_groups": ["my:saddled_on_ground"] }
          }
        ]
      },

      "my:unequip_saddle": {
        "sequence": [
          {
            "remove": {
              "component_groups": [
                "my:saddled",
                "my:saddled_in_water",
                "my:saddled_on_ground"
              ]
            }
          },
          { "add": { "component_groups": ["my:rideable"] } }
        ]
      },

      "my:saddled_enter_water": {
        "remove": { "component_groups": ["my:saddled_on_ground"] },
        "add": { "component_groups": ["my:saddled_in_water"] }
      },

      "my:saddled_leave_water": {
        "remove": { "component_groups": ["my:saddled_in_water"] },
        "add": { "component_groups": ["my:saddled_on_ground"] }
      },

      "my:start_tickle": {
        "set_property": { "my:is_tickled": true }
      },

      "my:end_tickle": {
        "set_property": { "my:is_tickled": false }
      },

      "my:start_roll": {
        "set_property": { "my:is_rolling": true }
      },

      "my:end_roll": {
        "set_property": { "my:is_rolling": false }
      },

      "my:start_clap": {
        "set_property": { "my:is_clapping": true }
      },

      "my:end_clap": {
        "set_property": { "my:is_clapping": false }
      },

      "my:anchor_seal": {
        "sequence": [
          { "set_property": { "my:is_anchored": true } },
          { "add": { "component_groups": ["my:tamed_anchored"] } },
          { "set_home_position": {} }
        ]
      },

      "my:free_seal": {
        "sequence": [
          { "set_property": { "my:is_anchored": false } },
          { "remove": { "component_groups": ["my:tamed_anchored"] } }
        ]
      },

      "my:grow_up": {
        "sequence": [
          { "remove": { "component_groups": ["my:baby"] } },
          {
            "add": {
              "component_groups": [
                "my:adult",
                "my:rideable",
                "my:tamed_inventory",
                "my:unchested"
              ]
            }
          }
        ]
      },

      "my:set_temperate_variant": {
        "sequence": [
          { "remove": { "component_groups": ["my:detect_biome"] } },
          {
            "randomize": [
              {
                "weight": 85,
                "add": { "component_groups": ["my:variant_gray"] }
              },
              {
                "weight": 15,
                "add": { "component_groups": ["my:variant_plant"] }
              }
            ]
          }
        ]
      },

      "my:set_warm_variant": {
        "sequence": [
          { "remove": { "component_groups": ["my:detect_biome"] } },
          { "add": { "component_groups": ["my:variant_brown"] } }
        ]
      },

      "my:set_cold_variant": {
        "sequence": [
          { "remove": { "component_groups": ["my:detect_biome"] } },
          { "add": { "component_groups": ["my:variant_arctic"] } }
        ]
      },

      "my:set_random_variant": {
        "sequence": [
          { "remove": { "component_groups": ["my:detect_biome"] } },
          { "add": { "component_groups": ["my:wild"] } },
          {
            "randomize": [
              {
                "weight": 35,
                "add": { "component_groups": ["my:variant_gray"] }
              },
              {
                "weight": 30,
                "add": { "component_groups": ["my:variant_arctic"] }
              },
              {
                "weight": 25,
                "add": { "component_groups": ["my:variant_brown"] }
              },
              {
                "weight": 10,
                "add": { "component_groups": ["my:variant_plant"] }
              }
            ]
          }
        ]
      }
    }
  }
}
