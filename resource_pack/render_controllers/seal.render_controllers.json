{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.seal": {
      "arrays": {
        "textures": {
          "Array.skin_variants": [
            "Texture.default",
            "Texture.arctic",
            "Texture.brown",
            "Texture.plant"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": ["Array.skin_variants[query.variant]"]
    },
    "controller.render.seal.saddle": {
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": ["Texture.saddle"],
      "part_visibility": [{ "*": false }, { "body": "query.is_saddled" }]
    }
  }
}
